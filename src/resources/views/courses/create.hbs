<div>
  <h1 class='my-4'>Dang Ky Khoa Hoc</h1>

  <form method='POST' action='/courses/store'>
    <div class='form-group mt-3'>
      <label for='name'>Ten khoa hoc</label>
      <input type='text' name='name' class='form-control mt-2' id='name' />
    </div>
    <div class='form-group mt-3'>
      <label for='description'>Mo ta</label>
      <input
        type='text'
        name='description'
        class='form-control mt-2'
        id='description'
      />
    </div>
    <div class='form-group mt-3'>
      <label for='videoId'>Video ID</label>
      <input
        type='text'
        name='videoId'
        class='form-control mt-2'
        id='videoId'
      />
    </div>
    <div class='form-group mt-3'>
      <label for='level'>Trinh do</label>
      <input type='text' name='level' class='form-control mt-2' id='level' />
    </div>
    <button type='submit' class='btn btn-primary mt-4'>Them khoa hoc</button>
  </form>
</div>